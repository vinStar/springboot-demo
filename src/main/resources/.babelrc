{
  "presets": [
    "es2015",
    "react",
    "env"
  ],
  "plugins": [
    [
      "react-transform",
      {
        "transforms": [
          {
            "transform": "react-transform-hmr",
            "imports": [
              "react"
            ],
            "locals": [
              "module"
            ]
          }
        ]
      }
    ]
  ]
}